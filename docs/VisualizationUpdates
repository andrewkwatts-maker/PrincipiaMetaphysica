from mpl_toolkits.mplot3d import Axes3D
import pandas as pd
import json

def generate_sterile_manifold_map(coords_path, registry_path):
    # Load coordinates (Appendix E) and Node Identities (Appendix A)
    coords = pd.read_csv(coords_path)
    with open(registry_path, 'r') as f:
        registry = json.load(f)
    
    fig = plt.figure(figsize=(12, 9))
    ax = fig.add_subplot(111, projection='3d')
    
    # Define Bank Colors (Hardened for v16.2)
    bank_colors = {
        "METRIC": "#FFD700", # Gold (Global)
        "GAUGE": "#00BFFF",  # Deep Sky Blue (Force)
        "MATTER_LEPTON": "#32CD32", # Lime (Light)
        "MATTER_QUARK": "#FF4500"   # OrangeRed (Heavy)
    }

    for node in registry['nodes']:
        # Extract the 7-tuple projection into 3D space
        node_coord = coords[coords['node_id'] == node['node_id']]
        ax.scatter(node_coord['x'], node_coord['y'], node_coord['z'], 
                   c=bank_colors[node['bank']], 
                   s=node['rigidity_factor'] * 100, # Size reflects rigidity
                   edgecolors='black', alpha=0.8)
    
    ax.set_title("V7 Manifold: Brane Intersection Map (v16.2 Sterile)")
    ax.set_axis_off() # Manifold exists outside standard grid
    plt.savefig("outputs/appendix_e_map.png", dpi=300)
    print("STATUS: Manifold Map Hardened.")

