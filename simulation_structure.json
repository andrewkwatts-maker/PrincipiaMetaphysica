{
  "version": "1.0",
  "description": "Simulation execution order and dependencies for Principia Metaphysica",
  "phases": [
    {
      "name": "Phase 1: Foundational Topology",
      "description": "G2 manifold structure and fundamental constants",
      "order": 1,
      "simulations": [
        {
          "name": "g2_metric_ricci_validation_v14_2",
          "file": "simulations/g2_metric_ricci_validation_v14_2.py",
          "inputs": [],
          "outputs": ["topology.B2", "topology.B3", "topology.CHI_EFF"],
          "description": "Validates G2 Ricci-flatness and extracts Betti numbers"
        },
        {
          "name": "g2_spinor_geometry_validation_v13_0",
          "file": "simulations/g2_spinor_geometry_validation_v13_0.py",
          "inputs": ["topology.B3"],
          "outputs": ["topology.parallel_spinor"],
          "description": "G2 parallel spinor validation"
        }
      ]
    },
    {
      "name": "Phase 2: Moduli Stabilization",
      "description": "Racetrack potential and modulus VEVs",
      "order": 2,
      "simulations": [
        {
          "name": "moduli_racetrack_stabilization_v15_0",
          "file": "simulations/moduli_racetrack_stabilization_v15_0.py",
          "inputs": ["topology.B3"],
          "outputs": ["pneuma.VEV", "dark_energy.lambda"],
          "description": "Racetrack modulus stabilization with flux"
        }
      ]
    },
    {
      "name": "Phase 3: Gauge Sector",
      "description": "GUT scale, coupling unification",
      "order": 3,
      "simulations": [
        {
          "name": "gauge_unification_precision_v12_4",
          "file": "simulations/gauge_unification_precision_v12_4.py",
          "inputs": ["pneuma.VEV"],
          "outputs": ["gauge.M_GUT", "gauge.ALPHA_GUT"],
          "description": "Gauge coupling unification at GUT scale"
        },
        {
          "name": "asymptotic_safety_rg_flow_v14_2",
          "file": "simulations/asymptotic_safety_rg_flow_v14_2.py",
          "inputs": ["gauge.ALPHA_GUT", "gauge.M_GUT"],
          "outputs": ["gauge.alpha_UV"],
          "description": "RG flow to UV fixed point"
        }
      ]
    },
    {
      "name": "Phase 4: Symmetry Breaking",
      "description": "SO(10) breaking chain",
      "order": 4,
      "simulations": [
        {
          "name": "breaking_chain_geometric_v14_1",
          "file": "simulations/breaking_chain_geometric_v14_1.py",
          "inputs": ["gauge.M_GUT", "pneuma.VEV"],
          "outputs": ["gauge.M_PS"],
          "description": "SO(10) → Pati-Salam → SM breaking"
        },
        {
          "name": "doublet_triplet_splitting_v14_0",
          "file": "simulations/doublet_triplet_splitting_v14_0.py",
          "inputs": ["gauge.M_GUT"],
          "outputs": ["gauge.doublet_triplet_ratio"],
          "description": "Doublet-triplet splitting mechanism"
        }
      ]
    },
    {
      "name": "Phase 5: Fermion Sector",
      "description": "Generations, masses, mixing",
      "order": 5,
      "simulations": [
        {
          "name": "fermion_chirality_generations_v13_0",
          "file": "simulations/fermion_chirality_generations_v13_0.py",
          "inputs": ["topology.CHI_EFF"],
          "outputs": ["topology.n_gen"],
          "description": "Three generations from G2 topology"
        },
        {
          "name": "g2_yukawa_overlap_integrals_v15_0",
          "file": "simulations/g2_yukawa_overlap_integrals_v15_0.py",
          "inputs": ["topology.B3", "pneuma.VEV"],
          "outputs": ["fermion.yukawa_matrix"],
          "description": "Yukawa couplings from G2 overlaps"
        }
      ]
    },
    {
      "name": "Phase 6: Neutrino Sector",
      "description": "PMNS matrix and masses",
      "order": 6,
      "simulations": [
        {
          "name": "pmns_full_matrix",
          "file": "simulations/pmns_full_matrix.py",
          "inputs": ["topology.B2", "topology.B3", "topology.n_gen"],
          "outputs": ["pmns.theta_12", "pmns.theta_23", "pmns.theta_13", "pmns.delta_CP"],
          "description": "Full PMNS matrix from topology"
        },
        {
          "name": "neutrino_mass_matrix_final_v12_7",
          "file": "simulations/neutrino_mass_matrix_final_v12_7.py",
          "inputs": ["gauge.M_GUT", "pmns.theta_12", "pmns.theta_23", "pmns.theta_13"],
          "outputs": ["neutrino.mass_spectrum"],
          "description": "Neutrino mass matrix via seesaw"
        }
      ]
    },
    {
      "name": "Phase 7: Higgs Sector",
      "description": "Higgs mass and VEV",
      "order": 7,
      "simulations": [
        {
          "name": "higgs_yukawa_rg_v12_4",
          "file": "simulations/higgs_yukawa_rg_v12_4.py",
          "inputs": ["pneuma.VEV", "gauge.M_GUT"],
          "outputs": ["higgs.m_H", "higgs.lambda_H"],
          "description": "Higgs mass from RG running"
        }
      ]
    },
    {
      "name": "Phase 8: Proton Decay",
      "description": "Proton lifetime prediction",
      "order": 8,
      "simulations": [
        {
          "name": "proton_decay_geometric_v13_0",
          "file": "simulations/proton_decay_geometric_v13_0.py",
          "inputs": ["gauge.M_GUT", "gauge.ALPHA_GUT"],
          "outputs": ["proton_decay.tau_p_years", "proton_decay.BR_epi0"],
          "description": "Proton lifetime with geometric suppression"
        }
      ]
    },
    {
      "name": "Phase 9: Cosmology",
      "description": "Dark energy and cosmological observables",
      "order": 9,
      "simulations": [
        {
          "name": "wz_evolution_desi_dr2",
          "file": "simulations/wz_evolution_desi_dr2.py",
          "inputs": ["pneuma.VEV", "dark_energy.lambda"],
          "outputs": ["dark_energy.w0", "dark_energy.wa"],
          "description": "Dark energy equation of state"
        }
      ]
    },
    {
      "name": "Phase 10: KK Spectrum",
      "description": "Kaluza-Klein tower predictions",
      "order": 10,
      "simulations": [
        {
          "name": "kk_spectrum_full",
          "file": "simulations/kk_spectrum_full.py",
          "inputs": ["topology.B2", "topology.B3"],
          "outputs": ["kk_spectrum.m1_TeV"],
          "description": "KK graviton mass spectrum"
        }
      ]
    },
    {
      "name": "Phase 11: Mirror Sector",
      "description": "Dark matter predictions",
      "order": 11,
      "simulations": [
        {
          "name": "multi_sector_sampling_v15_2",
          "file": "simulations/multi_sector_sampling_v15_2.py",
          "inputs": ["topology.HODGE_H11", "pneuma.VEV"],
          "outputs": ["mirror_sector.dm_baryon_ratio", "mirror_sector.temperature_ratio"],
          "description": "Multi-sector dark matter ratio"
        }
      ]
    }
  ],
  "metadata": {
    "total_phases": 11,
    "execution_mode": "sequential_by_phase",
    "param_chaining": true,
    "validation": "check_outputs_before_next_phase"
  }
}
